{
"manifest_version": 2,
    "name": "Diversearch",
    "description": "Plugin to change the page content from gendered to gender-neutral.",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}